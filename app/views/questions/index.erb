<h1>Poke Overflow Questions</h1>

<% if session[:user_id] %>
  <button id="get-question-form">
    Create New Question
  </button>
<% end %>

  <div id="new-question-form">
  </div>

<div id="questions">
  <ul class="view-single-question">
    <% @all_questions.each do |question| %>
      <%= erb :'partials/_questioninfo', locals: {question: question} %>
    <% end %>
  </ul>
</div>
