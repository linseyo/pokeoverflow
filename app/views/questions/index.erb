<h1>Poke Overflow Questions</h1>
  <button id="get-question-form">
    Create New Question
  </button>
  <div id="new-question-form">
  </div>

  <ul>
    <% @all_questions.each do |question| %>
      <li><a href="/questions/<%= question.id %>">
          <%= question.title %></a></li>
    <% end %>
  </ul>
